   upstream minio{{ item.name }} {
      least_conn;
      server 192.168.0.11:900{{ item.int }};
      server 192.168.0.12:900{{ item.int }};
      server 192.168.0.13:900{{ item.int }};
      server 192.168.0.14:900{{ item.int }};
      server 192.168.0.15:900{{ item.int }};
      server 192.168.0.16:900{{ item.int }};
   }

   server {
      listen 8{{ item.int }};

      location / {
          proxy_pass http://minio{{ item.name }};
      }
   }
